{
  "extends": "tslint:recommended",
  "rules": {
    "quotemark": [
      false
    ],
    "max-line-length": [
      false
    ],
    "interface-name": [
      false
    ],
    "no-console": [
      false
    ],
    "object-literal-sort-keys": false,
    "trailing-comma": [
      false
    ],
    "variable-name": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "max-classes-per-file": [
      false
    ],
    "align": [
      true,
      "statements"
    ],
    "triple-equals": [
      false
    ]
  }
}
